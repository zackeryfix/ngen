<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RTI.Ngen" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="include\;source\;doc\;" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/debug/RTI.Ngen" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-pg" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
					<Add directory="../external/lib/debug" />
					<Add directory="../bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="../bin/release/RTI.Ngen" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../external/lib/release" />
					<Add directory="../bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add option="-DNGEN_EXPORT" />
			<Add option="-DNGEN_REFLECTION_EXPORT" />
			<Add directory="../external/include" />
			<Add directory="../Ngen/include" />
			<Add directory="include" />
			<Add directory="../Reflection/include" />
		</Compiler>
		<Linker>
			<Add library="Ngen" />
			<Add library="Ngen.Reflection" />
			<Add directory="../external/lib" />
		</Linker>
		<Unit filename="LICENSE">
			<Option virtualFolder="doc/" />
		</Unit>
		<Unit filename="include/Build.Rti.Ngen.Logic.hpp">
			<Option virtualFolder="include/" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
